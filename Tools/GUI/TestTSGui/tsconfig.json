{
  "compilerOptions": {
    "outDir": "./dist/",
    "module": "es6",
    "moduleResolution": "node",
    "target": "es5",
    "jsx": "react",
    "inlineSourceMap": true,
    "inlineSources": true,
    "allowJs": true,
    "experimentalDecorators": true,
    "removeComments": false,
    "noImplicitAny": true,
    "suppressImplicitAnyIndexErrors": true,
    "allowSyntheticDefaultImports": true
  },
  "include": [
    "./src/**/*"
  ]
}